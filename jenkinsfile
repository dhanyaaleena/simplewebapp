#!/usr/bin/env groovy

@Library('shared-library@master') _ //master

pipeline{
	agent any
	
	stages{
		stage ('Compile Stage') {
			steps {
				withMaven(maven : 'maven-3.6.3'){
					sh 'mvn clean compile'
				}
			}
		}
		stage ('Testing Stage') {
			steps {
				withMaven(maven : 'maven-3.6.3'){
					sh 'mvn test'
				}
			}
		}
		stage ('Check logs') {
             steps {
                filterLogs ('WARNING', 1)
              }
         }
    } 
}
		